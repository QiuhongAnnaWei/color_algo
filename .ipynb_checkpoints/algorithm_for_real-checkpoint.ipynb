    "        return hsl.get_value_tuple()\n",
    "\n",
    "    def hsl_to_rgb(self, a, b, c):\n",
    "        hsl = HSLColor(a, b, c)\n",
    "        rgb = convert_color(hsl, sRGBColor).get_value_tuple()\n",
    "        if self.is_valid_rgb(rgb):\n",
    "            return [rgb[0]*255, rgb[1]*255, rgb[2]*255]\n",
