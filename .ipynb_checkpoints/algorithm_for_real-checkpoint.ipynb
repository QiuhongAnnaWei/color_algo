    "        return self.correct_rgb([rgb[0]*255, rgb[1]*255, rgb[2]*255])\n",
    "        \n",
    "    def is_valid_rgb(self, color):\n",
    "        for param in color:\n",
    "            if param < 0 or param > 255:\n",
    "                return False;\n",
    "        return True;\n",
    "    \n",
    "    def correct_rgb(self, color):\n",
    "        color = np.asarray(color)\n",
    "        for i in range(len(color)):\n",
    "            if color[i] < 0:\n",
    "                color[i] = 0\n",
    "            if color[i] > 255:\n",
    "                color[i] = 255\n",
    "        return color\n",
